TRANSFER{
Name(Акты инвентаризации)
Schema(COMPLEX)
Source(select   *
from     sdoc
where    type_sd = 530 and numref=:nref(i) and
         date() - save_sd <= :nday(i)
order by date_sd,type_sd,ndprt,autokey_sd)
SubSQL(select   *
from     sdspc
where    numref = $numref(i) and
         year = $year(i) and
         type_sd = $type_sd(i) and
         ndprt = $ndprt(i) and
         autokey_sd = $autokey_sd(i)
order by npp_spc)
Message($date_sd  Акт инвентаризации::  $ndprt / $autokey_sd)
Destination(process=REPLACE table=sdoc
columns(T:sklad;T:code_currency;N:flag))
Detail(process=FULL table=sdspc
condition(numref = $numref and year = $year and type_sd = $type_sd and
ndprt = $ndprt and autokey_sd = $autokey_sd)
columns(T:id_ware;proc_tax1=<prc_tax1;proc_tax2=<prc_tax2;proc_tax3=<prc_tax3))
}
TRANSFER{
Name(Акты прихода)
Schema(COMPLEX)
Source(select   *
from     sdoc
where    type_sd = 508 and numref=:nref(i) and
         date() - save_sd <= :nday(i)
order by date_sd,type_sd,ndprt,autokey_sd)
SubSQL(select   *
from     sdspc
where    numref = $numref(i) and
         year = $year(i) and
         type_sd = $type_sd(i) and
         ndprt = $ndprt(i) and
         autokey_sd = $autokey_sd(i)
order by npp_spc)
Message($date_sd  Акт прихода::  $ndprt / $autokey_sd)
Destination(process=REPLACE table=sdoc
columns(T:sklad;N:flag))
Detail(process=FULL table=sdspc
condition(numref = $numref and year = $year and type_sd = $type_sd and
ndprt = $ndprt and autokey_sd = $autokey_sd)
