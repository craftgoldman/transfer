#cli1_ndprt,#cli1_numb_client,#code_currency,^0,#sumf_sd,#date_sd,#flag);
update card_spc set cli_ndprt=#cli1_ndprt,
       numb_client=#cli1_numb_client
where  numref=$numref and year=$year and sklad=$sklad and
